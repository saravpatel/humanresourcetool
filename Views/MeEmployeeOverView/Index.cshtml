@model HRTool.Models.Me.MeEmployeeDetail_Overview
@{
    Layout = "~/Views/Shared/_MeLayout.cshtml";
}
<style>
  
</style>
 

<div  id="EmployeeInformation" class=" col-md-12">
    <input type="hidden" id="employeeId" value="@Model.id" />
    <table class="table table-responsive table-striped custab">
        <tbody>
            <tr>
                <td><b>Date of hire:</b></td>
                <td>@Model.DateOfHire</td>
                <td><b>Job Title:</b></td>
                <td>@Model.jobtitle</td>
            </tr>
            
            <tr>
                <td><b>Job Title:</b></td>
                <td>@Model.jobtitle</td>
                <td><b>Birthday:</b></td>
                <td>@Model.DOB</td>
            </tr>
           
            <tr>
                <td><b>Age:</b></td>
                <td>@Model.Age</td>
                <td><b>Mobile:</b></td>
                <td>@Model.mobile</td>
            </tr>
            
            <tr>
                <td><b>Email:</b></td>
                <td>@Model.emailId</td>
                <td><b>Gender:</b></td>
                <td>@Model.gender</td>
            </tr>
            
            <tr>
                <td><b>Work Phone:</b></td>
                <td>@Model.workPhone</td>
                <td><b>Country of Residence: </b></td>
                <td>@Model.CountryOfResidence</td>
            </tr>
            
            <tr>
                <td><b>Location:</b></td>
                <td>@Model.Location </td>
                <td><b> Nearest Airport:</b></td>
                <td> </td>
                </tr>
            
            <tr>
                <td> <b>Business:</b></td>
                <td> @Model.business</td>
                <td><b> Division:</b></td>
                <td>@Model.Division</td>
            </tr>

            

            <tr>
                <td><b>Pool:</b></td>
                <td>@Model.pool</td>
                <td><b>Function:</b></td>
                <td>@Model.FunctionName</td>
            </tr>

            @*<tr>
                <td><b>Report To:</b></td>
               
            </tr>*@
      </tbody>
    </table>


</div>




 
<div class="row" >
    
    <div class="col-md-12" style="padding-left: 25px;padding-right: 25px;">
    <div class="col-md-2 space-pad">
        <!-- BEGIN WIDGET THUMB -->
        <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
            <h4 class="widget-thumb-heading">Month Length Of Employment</h4>
            <div class="widget-thumb-wrap">
                <i class="widget-thumb-icon bg-green fa fa-arrow-right"></i>
                <div class="widget-thumb-body">
                    <span class="widget-thumb-subtitle">Length</span>
                    <span class="widget-thumb-body-stat" data-counter="counterup" data-value="7,644">@Model.employmentDay </span>
                </div>
            </div>
        </div>
        <!-- END WIDGET THUMB -->
    </div>

    <div class="col-md-2 space-pad">
        <!-- BEGIN WIDGET THUMB -->
        <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
            <h4 class="widget-thumb-heading">Days Absences</h4>
            <div class="widget-thumb-wrap">
                <i class="widget-thumb-icon bg-red fa fa-calendar"></i>
                <div class="widget-thumb-body">
                    <span class="widget-thumb-subtitle">Days</span>
                    <span class="widget-thumb-body-stat" data-counter="counterup" data-value="7,644">@Model.AbsencesDay</span>
                </div>
            </div>
        </div>
        <!-- END WIDGET THUMB -->
    </div>

    <div class="col-md-2 space-pad">
        <!-- BEGIN WIDGET THUMB -->
        <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
            <h4 class="widget-thumb-heading">days Holiday Remaining</h4>
            <div class="widget-thumb-wrap">
                <i class="widget-thumb-icon bg-purpel fa fa-users"></i>
                <div class="widget-thumb-body">
                    <span class="widget-thumb-subtitle">Holiday</span>
                    <span class="widget-thumb-body-stat" data-counter="counterup" data-value="7,644">@Model.toatalHolidayRemain</span>
                </div>
            </div>
        </div>
        <!-- END WIDGET THUMB -->
    </div>

    <div class="col-md-2 space-pad">
        <!-- BEGIN WIDGET THUMB -->
        <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
            <h4 class="widget-thumb-heading">days Contract Days</h4>
            <div class="widget-thumb-wrap">
                <i class="widget-thumb-icon bg-blue fa fa-address-card"></i>
                <div class="widget-thumb-body">
                    <span class="widget-thumb-subtitle">Contract</span>
                    <span class="widget-thumb-body-stat" data-counter="counterup" data-value="7,644">@Model.contractDays</span>
                </div>
            </div>
        </div>
        <!-- END WIDGET THUMB -->
    </div>

    <div class="col-md-2 space-pad">
        <!-- BEGIN WIDGET THUMB -->
        <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
            <h4 class="widget-thumb-heading">Days worked in Year</h4>
            <div class="widget-thumb-wrap">
                <i class="widget-thumb-icon bg-shine fa fa-desktop"></i>
                <div class="widget-thumb-body">
                    <span class="widget-thumb-subtitle">Work Days</span>
                    <span class="widget-thumb-body-stat" data-counter="counterup" data-value="7,644">@Model.workedDay</span>
                </div>
            </div>
        </div>
        <!-- END WIDGET THUMB -->
    </div>
    <div class="col-md-2 space-pad">
        <!-- BEGIN WIDGET THUMB -->
        <div class="widget-thumb widget-bg-color-white text-uppercase margin-bottom-20 bordered">
            <h4 class="widget-thumb-heading">Days worked since ever</h4>
            <div class="widget-thumb-wrap">
                <i class="widget-thumb-icon bg-black fa fa-briefcase"></i>
                <div class="widget-thumb-body">
                    <span class="widget-thumb-subtitle">since ever</span>
                    <span class="widget-thumb-body-stat" data-counter="counterup" data-value="7,644">@Model.DaysWorkedSinceEver </span>
                </div>
            </div>
        </div>
        <!-- END WIDGET THUMB -->
    </div>
</div>
</div>
<br/>
<div id="serdata" style="padding: 0px 0px 0px 0px;">
    <div class="accordion_container">

    </div>
</div>

        
<div id="MeOverviewLeaverTask" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div id="hideHeadder">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Add Employee Information</h4>
                </div>
            </div>
            <div class="modal-body" id="AddMeOverViewBody">
            </div>
            <div id="hideFooter">
                <div class="modal-footer" id="divfooter">
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/JS/Me/Overview/MeEmployeeOverViewJS.js"></script>
<script>
    var constantMeResources = {
        getResourceEmployeeInfo: '@Url.Action("getResourceEmployeeInfo", "MeEmployeeOverView")', 
        openLeaverWizard: '@Url.Action("getLeverWizardList", "MeEmployeeOverView")',
        getEmployeeBenefitData: '@Url.Action("getEmployeeBenefitData", "MeEmployeeOverView")',
        AddNewTask: '@Url.Action("AddNewTaskRecord", "MeEmployeeOverView")',
        ShowTaskData:'@Url.Action("ShowAddNewTask", "MeEmployeeOverView")',
    }

</script>