                    <li class="active">

                        @if (Model.IsSchedule)
                        {
                             <a id="ScheduleProjrctPlannerLink">Scheduling</a> 
                        }
                        else
                        {
                             <a id="ScheduleProjrctPlannerLink" onclick="GetAllSchedulingLeave(@Model.YearId,1,0,0,0)">Scheduling</a> 
                        }
                    </li>
                    <li>
