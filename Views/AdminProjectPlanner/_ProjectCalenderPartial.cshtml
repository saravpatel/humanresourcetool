<div id="ProjectCalanderMonth">
<input type="hidden" value="@IsSchdule" id="hdnSchedule" />
<input type="hidden" value="@IsTravel" id="hdnTravel"/>
<input type="hidden" value="@IsTimeSheet" id="hdnTimeSheet" />
<input type="hidden" value="@IsUplift" id="hdnUplift" />
