<input type="hidden" value="@Model.isExistWorkPatternLeave" id="isWorkpatternLeave" /><br/>
<div class="planner_calendar_header">
    <div class="changemonth">
        <a href="#" class="changemonth_previous" onclick="ChangePreviousMonth(@Model.YearId,@IsSchdule ,@IsTravel,@IsTimeSheet,@IsUplift)">
            <span class="fa fa-chevron-left"></span>
        </a>
        <span>January @Model.YearId - December @Model.YearId</span>
        <a href="#" class="changemonth_next" onclick="ChangeNextMonth(@Model.YearId,@IsSchdule ,@IsTravel,@IsTimeSheet,@IsUplift)">
            <span class="fa fa-chevron-right"></span>
        </a>
    </div>
</div>
