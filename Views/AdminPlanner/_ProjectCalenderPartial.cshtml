    <input type="hidden" value="@IsSickLeave" id="hdnSickLeave" />
    <input type="hidden" value="@IsLateLevae" id="hdnLateLevae" />
    <input type="hidden" value="@IsPublicHolidays" id="hdnPublicHolidays" />
    <input type="hidden" value="@IsMatPatLeave" id="hdnMatPatLeave" />
    <input type="hidden" value="@Model.IsTimesheetDrillDown" id="isDrillDown" /><br />
    <div class="planner_calendar_header">
        <div class="changemonth">
            <a href="#" class="changemonth_previous" onclick="ChangePreviousMonth(@Model.YearId,@IsTimeSheet,@IsTravel,@IsAnualLeave,@IsOtherLeave,@IsSickLeave,@IsLateLevae,@IsPublicHolidays,@IsPublicHolidays,@IsMatPatLeave)">
                <span class="fa fa-chevron-left"></span>
            </a>
            <span>January @Model.YearId - December @Model.YearId</span>
            <a href="#" class="changemonth_next" onclick="ChangeNextMonth(@Model.YearId,@IsTimeSheet,@IsTravel,@IsAnualLeave,@IsOtherLeave,@IsSickLeave,@IsLateLevae,@IsPublicHolidays,@IsPublicHolidays,@IsMatPatLeave)">
                <span class="fa fa-chevron-right"></span>
            </a>
        </div>
    </div>




