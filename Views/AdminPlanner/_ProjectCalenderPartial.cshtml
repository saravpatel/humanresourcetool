    var IsOtherLeave = Model.IsOtherLeave == true ? 1 : 0;
    var IsSickLeave = Model.IsSickLeave == true ? 1 : 0;
    var IsLateLevae = Model.IsLateLeave == true ? 1 : 0;
    var IsPublicHolidays = Model.IsPublicHolidays == true ? 1 : 0;
    var IsMatPatLeave = Model.IsMatPatLeave == true ? 1 : 0;
    var className = Model.IsTimeSheet ? "timeSheetIcon" : Model.IsTravel ? "travelSheetIcon" : Model.IsAnualLeave ? "anualLeaveIcon" : Model.IsOtherLeave ? "otherLeaveIcon"
