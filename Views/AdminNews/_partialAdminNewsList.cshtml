<input type="hidden" id="LoginUserId" value="@Model.Item1.UserId" />

<div id="AdminNewsModal" class="modal fade" role="dialog">
    <div class="modal-dialog modal-lg">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title adminNewsTitle">Add News</h4>
            </div>
            <div class="modal-body" id="AdminNewsBody">

            </div>
            <div class="modal-footer">
                <button type="submit" class="btn btn-success" id="btn-submit-AdminNews">Add</button>
                <button type="button" class="btn btn-warning" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>

<div class="table-btns">


    @if (@Model.Item1.AddEdit)
    {
        <a class="btn btn-success btn-circle btn-add-AdminNews" title="Add News" data-backdrop="static" data-keyboard="false" @*data-toggle="modal" data-target="#AdminNewsModal"*@><i class="fa fa-plus"></i></a>
        <a class="btn btn-primary btn-circle btn-edit-AdminNews" title="Edit News" data-backdrop="static" data-keyboard="false" @*data-toggle="modal" data-target="#AdminNewsModal"*@ disabled="disabled"><i class="fa fa-pencil"></i></a>
        <a class="btn btn-danger btn-circle btn-delete-AdminNews" data-toggle="tooltip" data-placement="bottom" title="Delete" disabled="disabled"><i class="fa fa-trash-o"></i> </a>
    }
    <a title="Refresh" class="btn btn-primary btn-circle btn-Refresh-AdminNews" data-toggle="tooltip" data-placement="bottom"><i class="fa fa-refresh"></i></a>
    @*<button title="Clear Sorting" class="btn btn-warning btn-circle  btn-ClearSorting-AdminNews" data-toggle="tooltip" data-placement="bottom"><i class="fa fa-upload"></i></button>*@
    <a class="btn btn-warning btn-circle btn-clearFilter-AdminNews" data-toggle="tooltip" data-placement="bottom" title="Clear Filter"><i class="fa fa-filter"></i></a>

