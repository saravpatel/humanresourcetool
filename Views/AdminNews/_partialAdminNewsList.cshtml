<table id="AdminNewsTable" class="table-bordered display table style2    dataTable" cellspacing="0">
    <thead>
        <tr class="tblHeading">
            <th style="padding:0px;"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model.Item2)
        {
            if (item.Super)
            {
                <tr id="@item.Id" class="noselect1 ng-scope dataTr">
                    <td colspan="4" class="newspage">
                        <div class="blog_list_teaser">
                            <ul class="md-list md-list-addon">
                                <li>
                                    <div class="md-list-addon-element">
                                        @if (!string.IsNullOrEmpty(item.CurrentImage))
                                        {
                                            <img src="~/Upload/Resources/@item.CurrentImage" class="md-user-image md-list-addon-avatar" />
                                        }
                                        else
                                        {
                                            <img class="md-user-image md-list-addon-avatar" src="~/img/avatars/avatar_02_tn.png" alt="">
                                        }

                                       </div>
                                    <div class="md-list-content">
                                        <span class="md-list-heading" style="color:black">@item.CreatedBy</span>
                                        <span class="uk-text-small uk-text-muted ng-binding">
                                            has posted a new <strong>update </strong>on - @item.CreatedDate. <!-- <a href="#" class="">1 Comments</a> -->
