@model List<Dictionary<string, object>>

<h2>Query Result</h2>
@if (Model.Count() > 0)
{
<div style="overflow:scroll;">    
<input type="hidden" class="form-control " value="@TempData["QueryText"]" id="txtQueryText">
    <table class="table ">
        @foreach (var row in Model.FirstOrDefault())
        {
            <th width="5%">@row.Key</th>
        }
        @foreach (var row in Model)
        {
            <tr width="5%">
                @foreach (var column in row)
                {
                    <td>@column.Value</td>
                }
            </tr>
        }
    </table>  
</div>
}
else
{
    <div>
        <h3>No record found</h3>
    </div>
}

